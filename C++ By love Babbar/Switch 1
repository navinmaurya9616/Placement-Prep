#include <iostream>
using namespace std;

int main() {
    //int n=2;
    char ch='a';
    cout<<endl;
    switch(ch)
    {
        case 1:
        cout<<"First ";
        break;
        case 2:
        cout<<"Second";
        break;
        Default:
        cout<<"Nothing";
        break;
        case 'a':
        cout<<"Charecter";
    }

    return 0;
}
