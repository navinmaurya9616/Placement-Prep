#include <iostream>
using namespace std;
bool isPrime(int n)
{
    for(int i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            return 0;
        }
    }
    return 1;
}

int main() {
    int n;
    cout<<"Enter The Number:";
    cin>>n;
    if(isPrime(n))
    {
        cout<<"is Prime Number.";
    }
    else
    {
        cout<<"is not a Prime Number .";
    }

    return 0;
}
